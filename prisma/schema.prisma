generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(cuid())
  phoneNumber String   @unique
  username    String?
  password    String
  avatarUrl   String?
  status      String?  @default("Hey there! I am using this app.")
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
